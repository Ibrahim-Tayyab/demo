{
  "version": 2,
  "buildCommand": "npm run build",
  "framework": "docusaurus",
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/chat",
      "destination": "/api/chat/handler.py"
    },
    {
      "source": "/api/health",
      "destination": "/api/health/handler.py"
    }
  ]
}
